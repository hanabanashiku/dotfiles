{
    "workbench.colorTheme": "Dracula Purple",
    "workbench.iconTheme": "vscode-icons",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "csharp.preview.improvedLaunchExperience": true,
    "vim.leader": "<space>",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "after": []
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        },
        {
            "before": [
                "<leader>",
                "q"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "n"
            ],
            "commands": [
                "workbench.action.file.newUntitledFile"
            ]
        },
        {
            "before": [
                "<leader>",
                "e"
            ],
            "commands": [
                "workbench.files.action.focusFilesExplorer"
            ]
        },
        {
            "before": [
                "<leader>",
                "h"
            ],
            "commands": [
                "workbench.action.splitEditorDown",
                "workbench.action.focusDownEditorGroup"
            ]
        },
        {
            "before": [
                "<leader>",
                "v"
            ],
            "commands": [
                "workbench.action.splitEditorRight",
                "workbench.action.focusRightEditorGroup"
            ]
        },
        {
            "before": [
                "L"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        },
        {
            "before": [
                "H"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "commands": [
                "workbench.action.focusRightEditorGroup"
            ]
        },
        {
            "before": [
                "<C-h>"
            ],
            "commands": [
                "workbench.action.focusLeftEditorGroup"
            ]
        },
        {
            "before": [
                "<C-j>"
            ],
            "commands": [
                "workbench.action.focusDownEditorGroup"
            ]
        },
        {
            "before": [
                "<C-k>"
            ],
            "commands": [
                "workbench.action.focusUpEditorGroup"
            ]
        },
        {
            "before": [
                "<C-d>"
            ],
            "after": [
                "<C-d>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-u>"
            ],
            "after": [
                "<C-u>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "g",
                "d"
            ],
            "after": [
                "g",
                "d",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<leader>",
                "s"
            ],
            "commands": [
                "editor.action.startFindReplaceAction"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "w"
            ],
            "commands": [
                "workbench.action.findInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "f"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "<leader>",
                "/"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.showDefinitionPreviewHover"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "J"
            ],
            "commands"[
                ":m '>+1<CR>gv=gv"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands"[
                ":m '<-2<CR>gv=gv"
            ]
        },
        {
            "before": [
                "p"
            ],
            "commands": [
                "pgv\"'.v:register.'y`>"
            ]
        },
        {
            "before": [
                "P"
            ],
            "commands": [
                "Pgv\"'.v:register.'y`>"
            ]
        },
        {
            "before": [
                "<"
            ],
            "after": [
                "<",
                "g",
                "v"
            ]
        },
        {
            "before": [
                ">"
            ],
            "after": [
                ">",
                "g",
                "v"
            ]
        },
        {
            "before": [
                "<leader>",
                "/"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        }
    ],
    "vim.surround": true,
    "editor.lineNumbers": "relative"
}